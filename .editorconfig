# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = crlf
insert_final_newline = true
indent_style = space
indent_size = 4

[*.{json,xml,html,hbs,config,props}]
indent_size = 2

[*.sh]
end_of_line = lf
insert_final_newline = false